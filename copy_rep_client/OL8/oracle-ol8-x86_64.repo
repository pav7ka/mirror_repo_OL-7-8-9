[ol8_oracle_instantclient]
name=Oracle Instant Client for Oracle Linux $releasever (x86_64)
#baseurl=https://yum.oracle.com/repo/OracleLinux/OL8/oracle/instantclient/x86_64/
baseurl=http://IP_OR_NAME/OL8/oracle-ol8-x86_64/ol8_oracle_instantclient
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-oracle
#gpgkey=http://IP_OR_NAME/rpm-gpg/RPM-GPG-KEY-oracle
gpgcheck=1
enabled=1
